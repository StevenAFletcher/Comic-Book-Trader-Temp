@model Project.Models.AddUser;
@{

    Layout = null;

}

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>ComicBook Trader</title>
</head>
<body>
    <link href="~/css/site.css" rel="stylesheet" />
    <table align="left">
        <tbody>
            <tr>
                <td><a asp-controller="Home" asp-action="Index">Home</a></td>
                <td><a asp-controller="Home" asp-action="Browse">Browse</a> </td>
                <td><a href="#">Shop</a> </td>
                <td><a href="#">Local Comics</a> </td>
            </tr>
        </tbody>
    </table>

    <table align="right">
        <tbody>
            <tr>
                <td><a href="#">Login</a></td>
                <td><a asp-controller="Home" asp-action="CreateAccount">Sign-up</a> </td>
                <td><a href="#">Help</a> </td>
                <td><a href="#">About</a> </td>
            </tr>
        </tbody>
    </table>
    <br />
    <p></p>

    <form method="post" align="middle">
        <div asp-validation-summary="All">

        </div>

        User Name:
        <input asp-for="UserName" />
        <br />
        First Name:
        <input asp-for="FirstName" />
        <br />

        Last Name:
        <input asp-for="LastName" />
        <br />

        Email Address:
        <input asp-for="EmailAddress" />
        <br />

        Password:
        <input asp-for="Password" />
        <br />

        Location:
        <input asp-for="Location" />
        <br />

        <input type="submit" nvalue="Submit" />

    </form>

</body>
</html>